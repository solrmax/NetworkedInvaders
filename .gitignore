# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/client/.utmp/
/client/[Ll]ibrary/
/client/[Tt]emp/
/client/[Oo]bj/
/client/[Bb]uild/
/client/[Bb]uilds/
/client/[Ll]ogs/
/client/[Uu]ser[Ss]ettings/
*.log

# By default unity supports Blender asset imports, *.blend1 blender files do not need to be commited to version control.
*.blend1
*.blend1.meta

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/client/[Mm]emoryCaptures/

# Recordings can get excessive in size
/client/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /client/[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/client/[Aa]ssets/Plugins/Editor/JetBrains*
# Jetbrains Rider personal-layer settings
*.DotSettings.user

# Visual Studio cache directory
.vs/

# Gradle cache directory
.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/client/ExportedObj/
/client/.consulo/
*.csproj
*.unityproj
*.sln
*.suo
*.tmp
*.user
*.userprefs
*.pidb
*.booproj
*.svd
*.pdb
*.mdb
*.opendb
*.VC.db

# Unity3D generated meta files
*.pidb.meta
*.pdb.meta
*.mdb.meta

# Unity3D generated file on crash reports
sysinfo.txt

# Mono auto generated files
mono_crash.*

# Builds
*.apk
*.aab
*.unitypackage
*.unitypackage.meta
*.app

# Crashlytics generated file
crashlytics-build.properties

# TestRunner generated files
InitTestScene*.unity*

# Addressables default ignores, before user customizations
/client/ServerData
/client/[Aa]ssets/StreamingAssets/aa*
/client/[Aa]ssets/AddressableAssetsData/link.xml*
/client/[Aa]ssets/Addressables_Temp*
# By default, Addressables content builds will generate addressables_content_state.bin
# files in platform-specific subfolders, for example:
# /client/Assets/AddressableAssetsData/OSX/addressables_content_state.bin
/client/[Aa]ssets/AddressableAssetsData/*/*.bin*

# Visual Scripting auto-generated files
/client/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Flow/UnitOptions.db
/client/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Flow/UnitOptions.db.meta
/client/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Core/Property Providers
/client/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Core/Property Providers.meta

# Auto-generated scenes by play mode tests
/client/[Aa]ssets/[Ii]nit[Tt]est[Ss]cene*.unity*

/server/package-lock.json
/server/node_modules/
.idea
/client/Library
/client/Logs
/client/.vsconfig
/client/UserSettings
*csproj
Temp
.vscode
